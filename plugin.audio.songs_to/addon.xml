<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<addon id="plugin.audio.songs_to" name="songs.to" version="1.0.0" provider-name="sphere">
  <requires>
    <import addon="xbmc.python" version="2.0"/>
    <import addon="script.module.xbmcswift2" version="1.3.0"/>
	<import addon="script.module.simplejson"/>

  </requires>
  <extension point="xbmc.python.pluginsource" library="addon.py">
    <provides>audio</provides>
  </extension>
  <extension point="xbmc.addon.metadata">
    <language></language>
    <platform>all</platform>
    <summary lang="en">Free Music Portal</summary>
    <description lang="en">With the songs.to XBMC Add-on you can listen to your favourite music. Current Features:[CR]- Search by artist, album, title or in all fields[CR]- Show all songs from a selected album or artist[CR]- create and manage as much playlists as you want and add songs to them[CR]- Find similar songs</description>
  </extension>
  <statfeed>
    <working>yes</working>
	<notes>none</notes>
	<maintainer>skatulskijean</maintainer>
    </statfeed>
</addon>
